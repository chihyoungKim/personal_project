/* body 마진 0으로 초기화 */
@import url('https://fonts.googleapis.com/css2?family=Gowun+Dodum&display=swap');
body {margin: 0;}
.font-gd {font-family: "Gowun Dodum";}

form button { padding: 0 10px; margin: 0 20px;
    background: #788c8d; color: #ecf0f1;border-radius: 5px; border-style: none;}
form button:hover:not(.btn-cf) {background: #667c7d;}

section {max-width: 978px; margin: 0 auto; min-height: 800px; padding: 20px;}

input[type=text], input[type=password] {border-radius: 5px;}
li:not(article > ul > li) {list-style: none; display: inline;}
li > * {text-decoration: none; color: #f4f7f7;}
.center {text-align: center;}
body .right {text-align: right;}
.overflow {white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
#btn-secession, #searchAddr, #btnEmail, #cfedEmail{padding: 0 10px;
  margin: 0 auto;
  background: #788c8d;
  color: #ecf0f1;
  border-radius: 5px;
  border-style: none;
  }
#searchAddr, #btnEmail, #cfedEmail{display: block;
  margin: auto;
  padding: 10px;}









/* header */
header {height: 160px; background: #f4f7f7;}
header nav:nth-child(1) {height: 100px;border-bottom: 2px solid #667c7d;}
header nav:nth-child(1) div {width: 978px; height: 140px; margin: auto;}
header nav:nth-child(1) div a {height: 100px; width: 116px;background: url('../assets/images/logo.png'); background-size: cover; float: left;}
header nav:nth-child(1) div::after {content: ' '; display: block; clear: both;}

header nav + nav {height: 60px;border-bottom: 2px solid #667c7d;}
header nav + nav ul.mainUl {width: 978px; margin: auto; padding-left: 0; display: block;}
header nav + nav ul.mainUl::after {content: ' '; display: block; clear: both;}
header nav + nav ul li.mainLi {padding: 10px 20px 10px 0; float: left;}
header nav + nav ul li.mainLi a {color: #788c8d; font-size: x-large; padding: 10px;}
header nav + nav ul li.mainLi:hover > a {color: #667c7d; font-weight: bold;}
header nav + nav ul.mainUl li ul {position: absolute; display: none; padding: 10px;}
header nav + nav ul.mainUl li:hover ul {display: block;}
header nav + nav ul.mainUl li ul li {position: relative; display: block;}
header nav + nav ul.mainUl li {margin: 0; background: #f4f7f7;}
header nav + nav ul.mainUl li ul {background: #f4f7f7; border-radius: 2px;}
header nav + nav ul.mainUl li ul li a {color: #788c8d; font-size: medium; padding: 2px;}
header nav + nav ul.mainUl li ul li a:hover {color: #667c7d; font-weight: bold; text-decoration: underline;}

/* footer */
footer {height: 160px; background: #f4f7f7;}
footer div {width: 978px; margin: auto;}
footer div div {padding-top: 20px;}
footer p {text-align: right; padding: 10px; font-style: italic; font-weight: bold; margin: 0; color: #788c8d;}

/* .background */
.background {background: #f4f7f7ee; min-height: 500px; margin: 50px auto; width: 700px; border-radius: 20px;}

/* signin and singup and write */
.background h2 {display: inline-block;}
.background > form > div:first-child {color: #667c7d; padding: 10px;}
.background > form > div div {padding: 5px; margin: 10px auto;}
.background > form > div div > label {width: 177px; font-weight: bold;}
.background > form > div div > *:not(#refund, #city, #time, #price, #file, #btns, #btn-join, .btns, #btn-secession, #searchAddr, #btnEmail, #cfedEmail) {display: block; margin: auto; padding: 10px; color: #788c8d;}
body .background > form > * {margin: auto;}
.background > form > div:nth-child(2) {min-height: 200px;}
.background > form > div:nth-child(2) > input {width: 400px; padding: 10px; margin: 10px auto;}
.background > form > div:nth-child(2) > input + textarea {width: 400px; height: 400px; padding: 10px; resize: none;}
.background > form > div + div + div {margin: 30px; padding: 30px 0;}
textarea.regText {width: 80%; height: 400px; padding: 10px; resize: none;}
.regText {width: 400px; padding: 10px; margin: 10px auto;}



/* notice and free */
.background table {width: 600px; margin: auto; border-collapse: collapse;}
.background table tr td > span {background: red; color: #ddd;}
.background table tr td a > span {color: red; font-weight: bold;}
.background table tr {border-bottom: 1px solid;}
.background table tr th {padding: 5px;}
.background table tr + tr:hover {background: #b7c2c3;}
.background table td, section div.background table td + td + td + td + td {width: 10%; text-align: center; padding: 5px;}
.background table td + td {width: 49%; text-align: left;}
.background table td + td > * {text-decoration: none; color: black;}
.background table td + td + td {width: 16%; text-align: center;}
.background .title {background: #667c7d; color: #f4f7f7; padding: 10px;}
.background table~nav {margin: 10px 50px 0 0;}
.background table~nav a {text-decoration: none; color: #f4f7f7; background: #667c7d; border-radius: 10px; padding: 10px; margin-left: 10px;}
.background table~nav a:hover {background: #788c8d99;}

/* detail */
.background div.clear input:not(#file) {float: left; padding: 10px; margin: 0 30px; width: 50%; margin-top: 20px;}
.background div h3 {float: left; padding: 10px; color: #f4f7f7; margin: 0 30px; width: 50%;}
.background input + h3 {float: right; width: 20%; padding-top: 20px; color: black;}
.background h3 + h3 {float: right; width: 20%;}
.background .title::after {content: ' '; display: block; clear: both;}
.background div.content-box {padding: 20px; margin: 20px auto; width: 80%; background: #eee;}
.background div.content2-box {padding: 10px; margin: 10px auto;}
.background .textarea {width: 400px; height: 100px; resize: none;}
.background div.title {width: 80%; margin: auto; padding: 20px;}
.background div:last-child ul li a, .background div:last-child ul li button {background: #667c7d; border-radius: 10px; padding: 10px; margin-left: 10px;}
.background div:last-child ul li a:hover, .background div:last-child ul li button:hover {background: #788c8d99;}
.background .modify {width: 400px; height: 400px; resize: none;}
.title h5 {margin: 10px 35px 0;}
.title .attach {text-decoration: none; color: #f4f7f7;}
.title .attach:hover {color: #E3C0B0;}

/* contract */
.background > form > div > input[type=checkbox] {padding: 50px; margin: 20px; width: auto;}
.background > form > div > span {color: red; font-size: small;}
.background > form > div.center {margin: 0; padding: 0; min-height: 0;}
.background > form > div.contract {margin: -5px 150px 10px 0; padding: 0; min-height: 0;}

/* index */
.main{float: left;}
.main {width: 69%;}
.aside {float: right; width: 27%; padding: 10px; margin: 0 auto;}
.aside > div {position: fixed;}
section::after, section div.main, section div.aside::after {content: ' '; display: block; clear: both;}
.aside {background: #788c8d;}
.aside nav ul {margin: 40px 0 0 0;}
.aside nav a, .aside nav p {text-decoration: none; color: #f4f7f7; padding: 20px;}
.aside nav p span {text-shadow: 2px 2px 2px black;}
.aside nav a:hover {text-shadow: 2px 2px 2px black;}
.aside nav ul li a {padding: 0 10px;}
.aside .center {padding: 10px;}
article, .aside {margin: 20px 0;}



/* saipan */
article:first-child p {font-size: x-large; display: inline-block; padding: 10px; margin-left: 20px; font-weight: bold; text-shadow: 0 0 2px black;}
article:first-child {color: #f4f7f7; animation: change-bg 5s ease 0.2s infinite alternate;}
article:first-child ul li {list-style: none; padding: 5px; font-size: large; font-weight: bold;}
article:first-child h1 {padding: 50px;}
article:first-child ul {text-align: right; padding: 10px;}
article:first-child a {padding: 10px; text-decoration: none; color: #f4f7f7; border: 1px #f4f7f7 solid; margin-left: 10px; border-radius: 20px;}
article:first-child nav {padding: 20px;}
@keyframes change-bg {
    from {
        background: url('../assets/images/saipan2.jpg'); background-size: cover;
    }
    to {
        background: url('../assets/images/saipan.jpg'); background-size: cover;
    }
}



/* domestic */
article:nth-child(2) nav a, .main article:nth-child(2) nav, .main article:nth-child(2) nav div {float: left;}
article:nth-child(2) nav::after, .main article:nth-child(2)::after {content: ' '; display: block; clear: both;}
article:nth-child(2) nav a {width: 130px; height: 90px; margin: 5px 0; overflow: hidden;}
article:nth-child(2) nav div {width: 200px; height: 90px;}
a:hover > img.domestic {transform: scale(1.1); transition-duration: .3s;}
article h2~nav div p {padding: 10px; margin-top: 0; font-size: 12px;}


/* immigration */
article:nth-child(3) nav {margin-top: 30px;}
article:nth-child(3) nav:last-child a {color: #788c8d; text-decoration: none; font-size: large; padding: 10px;}
article:nth-child(3) nav:last-child a:hover span {color: red; text-decoration: underline; font-weight: bold;}

/* international */
article:last-child > div {width: 670px; height: 200px; margin: 10px 0; overflow: hidden;}
article:last-child > nav > a {text-decoration: none; color: #bcc6c6;}
article:last-child > nav > a:hover {color: #788c8d; text-decoration: underline;}
article:last-child > div:hover img {transform: scale(1.02); transition-duration: .3s;}
article:last-child nav:first-child a {background: #f4f7f7; padding: 5px; border-radius: 50px; color: #667c7d; font-weight: bold;}
article:last-child nav:first-child {text-align: center; margin-top: 50px;}
article:last-child nav:first-child ul {padding: 0;}
article:last-child nav:first-child a:hover {background: #b7c2c3;}
article:last-child nav.center {margin-bottom: 30px;}

